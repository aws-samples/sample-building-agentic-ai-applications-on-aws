# AWS Configuration
AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your_access_key_id
# AWS_SECRET_ACCESS_KEY=your_secret_access_key

# AWS Secrets Manager Configuration
AWS_SECRET_NAME=mortgage_agents_ids

# Infrastructure Information
# App Runner Configuration
# APP_RUNNER_SERVICE_NAME=streamlit-bedrock-app
# APP_RUNNER_CPU=1
# APP_RUNNER_MEMORY=2
# APP_RUNNER_PORT=8501

# VPC Configuration (if needed)
# VPC_ID=vpc-xxxxxxxx
# SUBNET_IDS=subnet-xxxxxxxx,subnet-yyyyyyyy
# SECURITY_GROUP_IDS=sg-xxxxxxxx

# IAM Role (for App Runner service)
# IAM_ROLE_ARN=arn:aws:iam::123456789012:role/AppRunnerServiceRole

# CloudWatch Logs Configuration
# LOG_GROUP_NAME=/aws/apprunner/streamlit-bedrock-app

# Bedrock Agent Configuration (if not using Secrets Manager)
# BEDROCK_AGENT_ID_1=your-agent-id-1
# BEDROCK_AGENT_ALIAS_ID_1=your-agent-alias-id-1
# BEDROCK_AGENT_NAME_1=General Assistant

# BEDROCK_AGENT_ID_2=your-agent-id-2
# BEDROCK_AGENT_ALIAS_ID_2=your-agent-alias-id-2
# BEDROCK_AGENT_NAME_2=Data Analyst
